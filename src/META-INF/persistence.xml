<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="oficinaFXPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>modelo.Caixa</class>
    <class>modelo.Carro</class>
    <class>modelo.Cliente</class>
    <class>modelo.Fornecedor</class>
    <class>modelo.Itensservicos</class>
    <class>modelo.Mecanico</class>
    <class>modelo.Ordemservico</class>
    <class>modelo.Produto</class>
    <class>modelo.Produtofornecedor</class>
    <class>modelo.Servicos</class>
    <class>modelo.Telefone</class>
    <class>modelo.Produtoservico</class>
    <class>controlefx.Maquina</class>
    <class>controlefx.TipoServico</class>
    <class>controlefx.Usuario</class>
    <class>controlefx.Atendimento</class>
    <class>controlefx.TipoMaquina</class>
    <class>controlefx.Servico</class>
    <class>controlefx.Setor</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/oficina?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="mysql"/>
    </properties>
  </persistence-unit>
</persistence>
